						<td style="color:#1E7874;font-size:16px;border-bottom:1px solid black;">
							<strong>Product</strong>
						</td>
						<td class="text-center" style="color:#1E7874;font-size:16px;border-bottom:1px solid black;">
							<strong>Serial No</strong>
						</td>
                    </tr>
                     <tbody>
                        <tr t-foreach="line_item(o)" t-as="line_dc">
                            <td style="border-top:1px solid black;font-size:12px;position:relative">
                                <span t-esc="line_dc['product_name']"/>
                            </td>
                            <td style="border-top:1px solid black;font-size:12px;list-style-type:none;position:relative">
                                <li t-foreach="line_dc['lots']" t-as="lot">
                                    <span t-esc="lot"/>
                                </li>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </xpath>


<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record model="ir.ui.view" id="bbis_stock_production_lot_search">
            <field name="name">bbis.stock.lot.search</field>
            <field name="model">stock.lot</field>
            <field name="inherit_id" ref="stock.search_product_lot_filter"/>
            <field name="arch" type="xml">
                <field name="name" position="after">
                    <filter name="wh_stock" invisible="1" string="WH Stock" domain="[('quant_ids.location_id.name','=', 'Stock')]"/>
                </field>
            </field>
        </record>
    </data>
</odoo>
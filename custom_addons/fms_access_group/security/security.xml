<odoo>
    <data noupdate="0">
        <record model="ir.module.category" id="module_category_group_profile">
            <field name="name">Profiles</field>
            <field name="description">Helps you handle your groups in one place</field>
        </record>
        <!--        Sales User and Manager-->
        <record id="group_sale_profile_1" model="res.groups">
            <field name="name">Sales User</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user')),(4, ref('website_support.support_staff')),(4, ref('sales_team.group_sale_salesman'))]"/>
        </record>
        <record id="group_profile_sale_manager" model="res.groups">
            <field name="name">Sales Manager</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids"
                   eval="[(4, ref('base.group_user')),(4, ref('sales_team.group_sale_manager')),(4, ref('group_sale_profile_1')),(4, ref('website_support.support_staff'))]"/>
        </record>
        <!--        Purchase User and Manager-->
        <record id="group_profile_purchase_user" model="res.groups">
            <field name="name">Purchase User</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user')),(4, ref('purchase.group_purchase_user'))]"/>
        </record>
        <record id="group_profile_purchase_manager" model="res.groups">
            <field name="name">Purchase Manager</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user')),(4, ref('purchase.group_purchase_manager'))]"/>
        </record>
        <!--        Operation User and Manager-->
        <record id="group_profile_operation_user" model="res.groups">
            <field name="name">Operation User</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user')),(4, ref('fms_sale.group_engineer'))]"/>
        </record>
        <record id="group_profile_operation_manager" model="res.groups">
            <field name="name">Operation Manager</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user')),(4, ref('fms_sale.group_manager')),(4, ref('website_support.support_manager'))]"/>
        </record>
        <!--        Subscription User-->
        <record id="group_profile_subscription_user" model="res.groups">
            <field name="name">Subscription User</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids"
                   eval="[(4, ref('base.group_user')),(4, ref('website_support.support_staff')),(4, ref('sales_team.group_sale_salesman_all_leads'))]"/>
        </record>
        <record id="group_profile_subscription_manager" model="res.groups">
            <field name="name">Subscription Manager</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids"
                   eval="[(4, ref('base.group_user')),(4, ref('sales_team.group_sale_manager')),(4, ref('fms_sale.group_engineer')),(4, ref('sales_team.group_sale_salesman_all_leads')),(4, ref('website_support.support_manager'))]"/>
        </record>
        <!--Accountong User-->
        <record id="group_profile_accounting_user" model="res.groups">
            <field name="name">Accounting User</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids" eval="[(4, ref('account.group_account_invoice'))]"/>
        </record>
        <record id="group_profile_accounting_manager" model="res.groups">
            <field name="name">Accounting Manager</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids" eval="[(4, ref('account.group_account_manager'))]"/>
        </record>
        <!--        HR Users-->
        <record id="group_profile_hr_user" model="res.groups">
            <field name="name">HR User</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids"
                   eval="[(4, ref('hr.group_hr_user')),(4, ref('hr_attendance.group_hr_attendance_user'))]"/>
        </record>
        <record id="group_hr_payroll_director" model="res.groups">
            <field name="name">Director</field>
            <field name="category_id" ref="hr_payroll_community.module_category_hr_payroll_community"/>
            <field name="implied_ids" eval="[(4, ref('hr_payroll_community.group_hr_payroll_community_manager'))]"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>
        <record id="group_profile_hr_manager" model="res.groups">
            <field name="name">HR Manager</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids"
                   eval="[(4, ref('hr.group_hr_manager')),(4, ref('hr_attendance.group_hr_attendance_manager')),(4, ref('hr_holidays.group_hr_holidays_manager'))]"/>
        </record>
        <record id="group_profile_hr_director" model="res.groups">
            <field name="name">HR Director</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids"
                   eval="[(4, ref('group_profile_hr_manager'))]"/>
        </record>
        <!--        Inventory User and Manager-->
        <record id="group_profile_inventory_user" model="res.groups">
            <field name="name">Inventory User</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids" eval="[(4, ref('stock.group_stock_user'))]"/>
        </record>
        <record id="group_profile_inventory_manager" model="res.groups">
            <field name="name">Inventory Manager</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids"
                   eval="[(4, ref('stock.group_stock_manager')),(4, ref('group_profile_inventory_user'))]"/>
        </record>
        <!--        CSM User & Manager-->
        <record id="group_profile_csm_user" model="res.groups">
            <field name="name">CSM User</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids" eval="[(4, ref('website_support.support_staff')),(4, ref('sales_team.group_sale_salesman_all_leads')),(4, ref('sales_team.group_sale_salesman'))]"/>
        </record>
        <record id="group_profile_csm_manager" model="res.groups">
            <field name="name">CSM Manager</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user')),(4, ref('website_support.support_manager')),(4, ref('sales_team.group_sale_salesman_all_leads')),(4, ref('fms_sale.group_manager'))]"/>
        </record>
        <record id="group_profile_csm_director" model="res.groups">
            <field name="name">CSM Director</field>
            <field name="category_id" ref="module_category_group_profile"/>
            <field name="implied_ids" eval="[(4, ref('group_profile_csm_manager')),(4, ref('website_support.support_manager'))]"/>
        </record>
    </data>
</odoo>


<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

  <record id="repair_sequence" model="ir.sequence">
            <field name="name">Repair Order</field>
            <field name="code">repair.order</field>
            <field name="padding">4</field>
            <field name="prefix">REP/</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="task_sequence_inherit" model="ir.sequence">
            <field name="name">Task Sequence</field>
            <field name="code">work.order</field>
            <field name="padding">5</field>
            <field name="prefix">WORK/ORDER/</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>





    </data>
</odoo>